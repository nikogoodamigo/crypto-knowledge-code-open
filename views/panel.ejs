              <button type="submit" name="button" class="btn btn-outline-dark">Zobacz</button>
            </form>
          </div>
        </div>
      </div>
      <div class="col">
        <div class="card">
          <div class="card-header">
            Artukuły
          </div>
          <div class="card-body">
            <p>Udpostępnione wiadomości:</p>
            <p><%=numberOfArticles%></p>
            <form class="" action="/panel-news">
              <button type="submit" name="button" class="btn btn-outline-dark">Zobacz</button>
            </form>
          </div>
        </div>

      </div>
    </div>
  </div>
</div>

<%- include('partials/footer'); -%>
