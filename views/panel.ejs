    <div class="row">
      <div class="col">
        <div class="card">
          <div class="card-header">
            Kryptowaluty
          </div>
          <div class="card-body">
            <p>Udostępnione kryptowaluty:</p>
            <p><%=numberOfCryptos%></p>
